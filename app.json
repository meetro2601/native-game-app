{
  "expo": {
    "name": "madmoney",
    "slug": "madmoney",
    "version": "1.0.0",
    "plugins": ["@react-native-google-signin/google-signin"],
    "orientation": "portrait",
    "icon": "./assets/splash-screen.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-screen.png",
      "backgroundColor": "#ffffff",
      "imageResizeMode": "cover",
      "imageWidth": 200,
      "imageHeight": 100
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gopalrohra.madmoney"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gopalrohra.madmoney"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "91161c01-560d-4f72-9ba0-6de5facaed03"
      }
    }
  }
}
